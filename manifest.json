{
	"manifest_version": 3,
	"version": "0.0.6",
	"name": "Invidious Embedizer",
	"description": "Automatically replaces youtube embeds with invidious embeds in web pages",
	"options_page": "html/options.html",
	"action": {
		"default_title": "Embedize!!!",
		"default_icon": "images/icon.png"
	},
	"background": {
		"service_worker": "js/background.js"
	},
	"content_scripts": [
		{
			"matches": [ "<all_urls>" ],
			"js": ["js/replaceYoutube.js", "js/autoStart.js"]
		}
	],
	"permissions": ["storage", "activeTab", "scripting"],
	"icons": {
		"16": "images/icon.png",
		"32": "images/icon.png",
		"48": "images/icon.png",
		"128": "images/icon.png"
	}
}
